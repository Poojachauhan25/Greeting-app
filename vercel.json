{
  "version": 2,
  "builds": [
    {
      "src": "backendd/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/backendd/server.js" },
    { "src": "/(.*)", "dest": "/frontend/build/$1" }
  ]
}
